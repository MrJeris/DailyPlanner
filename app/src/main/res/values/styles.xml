<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MyTimePickerLight" parent="android:Widget.Material.Light.TimePicker">
        <item name="android:headerBackground">@color/black</item>
        <item name="android:numbersTextColor">@color/black</item>
        <item name="android:numbersSelectorColor">@color/black</item>
        <item name="android:numbersBackgroundColor">@color/gray</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="circle_image_profile" parent="">
        <item name="cornerSize">50%</item>
    </style>

    <style name="AlertDialogCustom" parent="Theme.AppCompat.Dialog.Alert">
        <item name="android:textColor">@color/white</item>
        <item name="android:headerDividersEnabled">true</item>
        <item name="android:background">@color/background_gray</item>
        <item name="android:textSize">18sp</item>
        <item name="titleCentered">true</item>
    </style>
</resources>